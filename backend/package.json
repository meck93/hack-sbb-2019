{
  "name": "test-back",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.graphql\" \"src/**/*.json\""
  },
  "dependencies": {
    "@types/jsonwebtoken": "8.3.4",
    "axios": "0.19.0",
    "bcryptjs": "2.4.3",
    "dotenv": "8.1.0",
    "faunadb": "2.8.1",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "10.14.19",
    "dotenv-cli": "2.0.1",
    "husky": "3.0.5",
    "lint-staged": "9.4.0",
    "nodemon": "1.19.2",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "ts-node": "8.4.1",
    "typescript": "3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.graphql": [
      "prettier --write",
      "git add"
    ]
  }
}
