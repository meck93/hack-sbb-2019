    "@types/node": "12.7.8",
    "@types/rc-slider": "^8.6.5",
    "@types/react": "16.9.3",
    "@types/react-datepicker": "^2.9.2",
    "@types/react-dom": "16.9.1",
    "@types/react-lottie": "^1.2.3",
    "@types/react-router-dom": "^5.1.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.4.0",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "rc-slider": "^8.7.1",
    "react": "^16.10.0",
    "react-datepicker": "^2.9.6",
    "react-dom": "^16.10.0",
    "react-lottie": "^1.2.3",
    "react-pose": "^4.0.8",
    "react-router-dom": "^5.1.1",
    "react-scripts": "3.1.2",
    "semantic-ui-react": "^0.88.1",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.graphql\" \"src/**/*.json\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "git add"
    ],
    "*.tsx": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.graphql": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.5.0",
    "@semantic-ui-react/craco-less": "^1.1.0",
    "semantic-ui-less": "^2.4.1"
  }
}
